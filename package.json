{
  "name": "universal-input-processor",
  "version": "1.0.0",
  "description": "AI-powered universal input processing API with dynamic tool orchestration",
  "main": "dist/server.js",
  "type": "module",
  "scripts": {
    "dev": "tsx watch src/server.ts",
    "build": "tsc",
    "start": "node dist/server.js",
    "kill-port": "lsof -ti:3000 | xargs kill -9 2>/dev/null || echo 'Port 3000 jest wolny'",
    "test": "vitest",
    "test:unit": "vitest run --dir tests/unit",
    "test:integration": "vitest run --dir tests/integration",
    "lint": "eslint src --ext .ts",
    "format": "prettier --write \"src/**/*.ts\""
  },
  "keywords": [
    "fastify",
    "ai",
    "llm",
    "orchestration",
    "pipeline",
    "typescript"
  ],
  "author": "Your Name",
  "license": "MIT",
  "dependencies": {
    "fastify": "^4.26.0",
    "@fastify/multipart": "^8.1.0",
    "@fastify/cors": "^9.0.1",
    "@fastify/rate-limit": "^9.1.0",
    "@anthropic-ai/sdk": "^0.17.0",
    "openai": "^4.28.0",
    "pdf-parse": "^1.1.1",
    "sharp": "^0.33.2",
    "axios": "^1.6.7",
    "zod": "^3.22.4",
    "dotenv": "^16.4.1",
    "pino": "^8.19.0",
    "pino-pretty": "^10.3.1"
  },
  "devDependencies": {
    "typescript": "^5.3.3",
    "@types/node": "^20.11.5",
    "@types/pdf-parse": "^1.1.4",
    "tsx": "^4.7.0",
    "vitest": "^1.2.1",
    "@vitest/ui": "^1.2.1",
    "eslint": "^8.56.0",
    "@typescript-eslint/eslint-plugin": "^6.19.1",
    "@typescript-eslint/parser": "^6.19.1",
    "prettier": "^3.2.4"
  },
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=9.0.0"
  }
}